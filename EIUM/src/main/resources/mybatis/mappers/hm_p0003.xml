<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
      PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.hm_p0003">
	<resultMap id="P0003resultVO" type="HM_P0003VO">
		<result property="admin_CODE" column="ADMIN_CODE" />
		<result property="admin_NAME" column="ADMIN_NAME" />
		<result property="admin_LIST_NAME" column="ADMIN_LIST_NAME" />
   
	</resultMap> 
	<select id="searchList" resultMap="P0003resultVO" parameterType="java.util.Map"> 
      <![CDATA[
         select ADMIN_CODE,ADMIN_NAME,ADMIN_LIST_NAME           
         from ADMIN
      ]]>
	</select>
	
	
	<resultMap id="POSITION_P0003resultVO" type="HM_P0003VO">
		<result property="admin_LIST_CODE" column="POSITION_CODE" />
		<result property="admin_LIST_NAME" column="POSITION_NAME" />
	</resultMap>
 
	
	<select id="POSITION_searchList" resultMap="POSITION_P0003resultVO" parameterType="java.util.Map"> 
         select POSITION_CODE,POSITION_NAME         
         from POSITION
	</select>
	
	<update id="POSITION_updateData" parameterType="java.util.Map">
		update POSITION
		set  
		     POSITION_NAME = #{admin_LIST_NAME}
		where TRIM(POSITION_CODE) = #{admin_LIST_CODE}
	</update>	
	
 <insert id="POSITION_insertData" parameterType="java.util.Map">
		insert into POSITION(POSITION_CODE,POSITION_NAME) 
		values(#{admin_LIST_CODE},#{admin_LIST_NAME})
	</insert>	
	
	<delete id="POSITION_deleteData" parameterType="java.util.Map">
		delete POSITION
		where TRIM(POSITION_CODE) = #{admin_LIST_CODE}
	</delete>


</mapper>